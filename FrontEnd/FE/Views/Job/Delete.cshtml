@model FE.Models.Job

@{
    ViewData["Title"] = "Delete";
}

<h3>¿Esta seguro que desea eliminar?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
Trabajo:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DescripcionJob)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdJob" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver</a>
    </form>
</div>
